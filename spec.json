{
  "nio/FirebaseInsert": {
    "version": "1.0.0",
    "description": "The Firebase Insert block takes incoming signals and saves them to the Firebase database under the specified `collection`. If no `collection` is specified, each signal will be saved in the database under a uniquely generated key.",
    "properties": {
      "application": {
        "title": "Firebase Application",
        "type": "StringType",
        "description": "Firebase url",
        "default": "[[FIREBASE_APPLICATION]]"
      },
      "auth": {
        "title": "Authentication",
        "type": "ObjectType",
        "description": "Firebase user id and app secret",
        "default": {
          "user_id": null,
          "app_secret": null
        }
      },
      "collection": {
        "title": "Firebase Collection",
        "type": "StringType",
        "description": "Object key for accessing elements in the database tree",
        "default": "/"
      },
      "enrich": {
        "title": "Signal Enrichment",
        "type": "ObjectType",
        "description": "Options for enriching signals.",
        "default": {
          "enrich_field": "",
          "exclude_existing": true
        }
      }
    },
    "inputs": {
      "default": {
        "description": "Any list of signals."
      }
    },
    "outputs": {
      "default": {
        "description": "The result of the insert."
      }
    },
    "commands": {}
  },
  "nio/FirebaseRead": {
    "version": "1.0.0",
    "description": "The Firebase Read block takes incoming signals and uses them to read from the Firebase database under the specified `collection`.",
    "properties": {
      "application": {
        "title": "Firebase Application",
        "type": "StringType",
        "description": "Firebase url",
        "default": "[[FIREBASE_APPLICATION]]"
      },
      "auth": {
        "title": "Authentication",
        "type": "ObjectType",
        "description": "Firebase user id and app secret",
        "default": {
          "user_id": null,
          "app_secret": null
        }
      },
      "collection": {
        "title": "Firebase Collection",
        "type": "StringType",
        "description": "Object key for accessing elements in the database tree",
        "default": "/"
      },
      "enrich": {
        "title": "Signal Enrichment",
        "type": "ObjectType",
        "description": "Options for enriching signals.",
        "default": {
          "enrich_field": "",
          "exclude_existing": true
        }
      }
    },
    "inputs": {
      "default": {
        "description": "Any list of signals."
      }
    },
    "outputs": {
      "default": {
        "description": "Results of the read."
      }
    },
    "commands": {}
  }
}